find ./*.ts | xargs cat | tee -a ../catfile.txt
cd ..
cd repositories
find ./*.ts | xargs cat | tee -a ../catfile.txt
cd ..
cd routers
find ./*.ts | xargs cat | tee -a ../catfile.txt
cd ..
cd services
find ./*.ts | xargs cat | tee -a ../catfile.txt
cd ..
cat -n catfile.txt catfile.txt
